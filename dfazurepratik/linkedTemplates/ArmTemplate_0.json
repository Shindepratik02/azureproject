						"name": "LoadNewData",
						"type": "IfCondition",
						"dependsOn": [
							{
								"activity": "SqlToLake",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"userProperties": [],
						"typeProperties": {
							"expression": {
								"value": "@greater(activity('SqlToLake').output.dataRead,0)",
								"type": "Expression"
							},
							"ifFalseActivities": [
								{
									"name": "Deleterow",
									"type": "Delete",
